{
    "name": "@nick-mazuk/ui-svelte",
    "version": "0.0.0-development",
    "type": "module",
    "scripts": {
        "dev": "start-storybook -p 6006 --quiet",
        "build": "build-storybook -c .storybook -o docs",
        "prebuild": "npm run generate-docs",
        "check-types": "tsc --noemit",
        "prettier": "prettier --write . --plugin-search-dir=.",
        "eslint": "eslint . --quiet",
        "svelte-check": "svelte-check",
        "lint": "npm run prettier && npm run svelte-check && npm run eslint && npm run check-types",
        "create-component": "./node_modules/.bin/esbuild ./cli/create-new-component --outfile=cli/dist/create-new-component.mjs --platform=node --target=node12.17 && node cli/dist/create-new-component.mjs",
        "build-icons": "./node_modules/.bin/esbuild ./cli/build-icons --outfile=cli/dist/build-icons.mjs --platform=node --target=node12.17 && node cli/dist/build-icons.mjs",
        "generate-docs": "./node_modules/.bin/esbuild ./cli/generate-docs/index.ts --outfile=cli/dist/generate-docs.js  --bundle --platform=node --target=node12.17 && node cli/dist/generate-docs.js",
        "prepublish": "npm run build-icons && npm run generate-docs",
        "cy:open": "cypress open",
        "cy:run": "cypress run",
        "semantic-release": "semantic-release"
    },
    "devDependencies": {
        "@babel/core": "^7.13.13",
        "@babel/preset-typescript": "^7.13.0",
        "@nick-mazuk/eslint-config": "^0.8.11",
        "@nick-mazuk/lib": "^0.8.50",
        "@nick-mazuk/semantic-release-config": "^1.1.3",
        "@nick-mazuk/ui-config": "^0.10.61",
        "@semantic-release/changelog": "^5.0.1",
        "@semantic-release/git": "^9.0.0",
        "@storybook/addon-a11y": "^6.2.9",
        "@storybook/addon-essentials": "^6.2.9",
        "@storybook/addon-links": "^6.2.9",
        "@storybook/addon-postcss": "^2.0.0",
        "@storybook/addon-svelte-csf": "^1.0.0",
        "@storybook/addons": "^6.2.9",
        "@storybook/svelte": "^6.2.9",
        "@storybook/theming": "^6.2.9",
        "@types/diff": "^5.0.0",
        "@types/feather-icons": "^4.7.0",
        "@types/fs-extra": "^9.0.11",
        "@types/marked": "^2.0.0",
        "@types/node": "^15.0.2",
        "@types/svg-parser": "^2.0.1",
        "@types/validator": "^13.1.3",
        "ac-colors": "^1.4.2",
        "autoprefixer": "^10.2.5",
        "axe-core": "^4.2.0",
        "chromatic": "^5.8.0",
        "concurrently": "^6.0.2",
        "cypress": "^7.3.0",
        "cypress-axe": "^0.12.2",
        "cypress-plugin-tab": "^1.0.5",
        "cypress-real-events": "^1.4.0",
        "esbuild": "^0.12.0",
        "eslint": "^7.26.0",
        "feather-icons": "^4.28.0",
        "fs-extra": "^10.0.0",
        "hast-util-to-html": "^8.0.0",
        "postcss": "^8.2.8",
        "prettier": "^2.2.1",
        "prettier-plugin-svelte": "^2.2.0",
        "semantic-release": "^17.4.2",
        "storybook-addon-outline": "^1.2.0",
        "storybook-addon-pseudo-states": "^1.0.0",
        "storybook-addon-themes": "^6.0.1",
        "storybook-addon-turbo-build": "^1.0.0-beta",
        "svelte": "^3.35.0",
        "svelte-check": "^1.3.0",
        "svelte-loader": "^3.1.1",
        "svelte-preprocess": "^4.7.0",
        "svg-parser": "^2.0.4",
        "tailwindcss": "^2.1.1",
        "ts-loader": "^9.1.2",
        "typescript": "^4.2.4"
    },
    "keywords": [
        "svelte"
    ],
    "files": [
        "src/",
        ".npmignore",
        "types.d.ts"
    ],
    "peerDependencies": {
        "@nick-mazuk/lib": "^0.8.50",
        "@nick-mazuk/ui-config": "^0.10.61",
        "tailwindcss": "^2.1.1"
    },
    "dependencies": {
        "@tiptap/core": "^2.0.0-beta.62",
        "@tiptap/extension-blockquote": "^2.0.0-beta.12",
        "@tiptap/extension-bold": "^2.0.0-beta.12",
        "@tiptap/extension-bullet-list": "^2.0.0-beta.12",
        "@tiptap/extension-character-count": "^2.0.0-beta.12",
