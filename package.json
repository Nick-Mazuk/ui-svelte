{
    "name": "@nick-mazuk/ui-svelte",
    "version": "0.1.26",
    "type": "module",
    "scripts": {
        "test": "jest test ./src --json --outputFile=.jest-test-results.json",
        "test:watch": "npm test -- --watch",
        "dev": "start-storybook -p 6006",
        "test:generate-output": "jest --json --outputFile=.jest-test-results.json",
        "build": "npm run test:generate-output; build-storybook -c .storybook -o docs",
        "check-types": "tsc --noemit",
        "prettier": "prettier --write . --plugin-search-dir=.",
        "lint": "npm run prettier && eslint . --quiet --fix && npm run check-types",
        "create-component": "./node_modules/.bin/esbuild cli/create-new-component.ts --outfile=cli/dist/create-new-component.js --platform=node --target=node12.17 && node cli/dist/create-new-component.js"
    },
    "devDependencies": {
        "@babel/core": "^7.13.13",
        "@babel/preset-typescript": "^7.13.0",
        "@nick-mazuk/eslint-config": "^0.7.44",
        "@nick-mazuk/lib": "^0.8.34",
        "@nick-mazuk/ui-config": "0.8.31",
        "@storybook/addon-a11y": "^6.2.0-rc.10",
        "@storybook/addon-actions": "^6.2.0-rc.10",
        "@storybook/addon-essentials": "^6.2.0-rc.10",
        "@storybook/addon-jest": "^6.1.21",
        "@storybook/addon-links": "^6.2.0-rc.9",
        "@storybook/addon-postcss": "^2.0.0",
        "@storybook/addon-svelte-csf": "^1.0.0",
        "@storybook/addons": "^6.2.0-rc.10",
        "@storybook/svelte": "^6.2.0-rc.10",
        "@storybook/theming": "^6.2.0-rc.10",
        "@tailwindcss/aspect-ratio": "^0.2.0",
        "@tailwindcss/forms": "^0.3.2",
        "@tailwindcss/jit": "^0.1.8",
        "@tailwindcss/line-clamp": "^0.2.0",
        "@tailwindcss/typography": "^0.4.0",
        "@testing-library/jest-dom": "^5.11.10",
        "@testing-library/svelte": "^3.0.3",
        "@types/fs-extra": "^9.0.9",
        "@types/jest": "^26.0.22",
        "@types/node": "^14.14.36",
        "autoprefixer": "^10.2.5",
        "eslint": "^7.23.0",
        "fs-extra": "^9.1.0",
        "jest": "^26.6.3",
        "postcss": "^8.2.8",
        "prettier": "^2.2.1",
        "prettier-plugin-svelte": "^2.2.0",
        "storybook-addon-themes": "^6.0.1",
        "storybook-dark-mode": "^1.0.8-canary.49.3230.0",
        "svelte": "^3.35.0",
        "svelte-jester": "^1.3.2",
        "svelte-loader": "^3.0.0",
        "svelte-preprocess": "^4.7.0",
        "tailwindcss": "^2.0.4",
        "ts-jest": "^26.5.4"
    },
    "keywords": [
        "svelte"
    ],
    "files": [
        "src/",
        ".npmignore"
    ],
    "peerDependencies": {
        "@nick-mazuk/lib": "^0.8.34",
        "@nick-mazuk/ui-config": "0.8.24",
        "@tailwindcss/aspect-ratio": "^0.2.0",
        "@tailwindcss/forms": "^0.3.0",
        "@tailwindcss/line-clamp": "^0.2.0",
        "@tailwindcss/typography": "^0.4.0",
        "tailwindcss": "^2.0.4"
    },
    "dependencies": {
        "dompurify": "^2.2.7",
        "esbuild": "^0.11.0",
        "marked": "^2.0.1"
    }
}
